<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
		<Nullable>disable</Nullable>
		<TargetFrameworks>$(TargetFrameworks)</TargetFrameworks>
		<RootNamespace>BlackbirdSql.Data</RootNamespace>
		<EnableDefaultCompileItems>false</EnableDefaultCompileItems>
		<EnableDefaultPageItems>false</EnableDefaultPageItems>
		<EnableDefaultEmbeddedResourceItems>false</EnableDefaultEmbeddedResourceItems>
		<SignAssembly>True</SignAssembly>
		<AssemblyOriginatorKeyFile>Properties\BlackbirdSql.Data.snk</AssemblyOriginatorKeyFile>
		<PlatformTarget>x64</PlatformTarget>
		<AllowUnsafeBlocks>true</AllowUnsafeBlocks>
		<ApplicationIcon>blackbirdsql.ico</ApplicationIcon>
	</PropertyGroup>

	<PropertyGroup Condition="'$(Configuration)|$(TargetFramework)|$(Platform)'=='Debug|net48|AnyCPU'">
		<NoWarn>1701;1702;CS0618</NoWarn>
	</PropertyGroup>

	<PropertyGroup Condition="'$(Configuration)|$(TargetFramework)|$(Platform)'=='Release|net48|AnyCPU'">
		<NoWarn>1701;1702;CS0618</NoWarn>
	</PropertyGroup>

	<ItemGroup>
	  <None Remove="blackbirdsql.ico" />
	</ItemGroup>

	<ItemGroup>
	  <Compile Include="DbConnectionService.cs" />
	  <Compile Include="DbCommandService.cs" />
	  <Compile Include="DatabaseInfoService.cs" />
	  <Compile Include="Ctl\DbNativeStringParser.cs" />
	  <Compile Include="DatabaseEngineService.cs" />
	  <Compile Include="DbConstants.cs" />
	  <Compile Include="GlobalSuppressions.cs" />
	  <Compile Include="LibraryData.cs" />
	  <Compile Include="DbExceptionService.cs" />
	  <Compile Include="Model\BinaryEncoding.cs" />
	  <Compile Include="Model\Charset.cs" />
	  <Compile Include="Model\DbBatchParser.cs" />
	  <Compile Include="Model\DbConnectionWrapper.cs" />
	  <Compile Include="Model\DbIscCodes.cs" />
	  <Compile Include="Model\DbStatementWrapper.cs" />
	  <Compile Include="Model\DbTypeHelper.cs" />
	  <Compile Include="Properties\InternalsVisibleToAssemblyInfo.cs" />
	  <Compile Include="Properties\Resources.Designer.cs">
	    <DesignTime>True</DesignTime>
	    <AutoGen>True</AutoGen>
	    <DependentUpon>Resources.resx</DependentUpon>
	  </Compile>
	</ItemGroup>

	<ItemGroup>
	  <Content Include="blackbirdsql.ico" />
	</ItemGroup>

	<ItemGroup>
	  <EmbeddedResource Include="Properties\Resources.resx">
	    <Generator>ResXFileCodeGenerator</Generator>
	    <LastGenOutput>Resources.Designer.cs</LastGenOutput>
	  </EmbeddedResource>
	</ItemGroup>

	<ItemGroup>
	  <PackageReference Include="FirebirdSql.Data.FirebirdClient" Version="10.1.0" />
	  <PackageReference Include="Microsoft.VisualStudio.Data" Version="17.5.33428.388" />
	</ItemGroup>

	<ItemGroup>
	  <ProjectReference Include="..\BlackbirdSql.Sys\BlackbirdSql.Sys.csproj" />
	</ItemGroup>


</Project>
