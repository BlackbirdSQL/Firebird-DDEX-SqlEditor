	</Define>

	<!-- Definition of index node column properties -->
	<Define name="ElementIndexColumnProperties" >
		<Properties>
			<TypeDisplayName resource="Type_IndexColumn" />
			<Property name="TABLE_CATALOG">
				<DisplayName resource="Identifier_Name_Catalog" />
				<Category resource="Category_Identifier" />
				<Description resource="Identifier_Desc_Catalog" />
			</Property>
			<Property name="TABLE_SCHEMA">
				<DisplayName resource="Identifier_Name_Catalog" />
				<Category resource="Category_Identifier" />
				<Description resource="Identifier_Desc_Catalog" />
			</Property>
			<Property name="TABLE_NAME">
				<DisplayName resource="Identifier_Name_Table" />
				<Category resource="Category_Identifier" />
				<Description resource="Identifier_Desc_Table" />
			</Property>
			<Property name="INDEX_NAME">
				<DisplayName resource="Identifier_Name_Index" />
				<Category resource="Category_Identifier" />
				<Description resource="Identifier_Desc_Index" />
			</Property>
			<Property name="COLUMN_NAME">
				<DisplayName resource="Identifier_Name_Name" />
				<Category resource="Category_Identifier" />
				<Description resource="Identifier_Desc_Name" />
			</Property>
			<Property name="ORDINAL_POSITION">
				<DisplayName resource="Property_Name_UniqueKeyColumn_Ordinal" />
				<Description resource="Property_Name_UniqueKeyColumn_Ordinal" />
			</Property>
			<!--
			<Property name="COLLATION">
				<DisplayName resource="Property_Name_IndexColumn_Collation" />
				<Description resource="Property_Desc_IndexColumn_Collation" />
				<StandardValues>
					<StandardValue value="1">ASC</StandardValue>
					<StandardValue value="2">DESC</StandardValue>
				</StandardValues>
			</Property>
			-->
			<Property name="COMPUTED">
				<DisplayName resource="Property_Name_Column_Computed" />
				<Description resource="Property_Desc_Column_Computed" />
			</Property>
			<Property name="EXPRESSION">
				<DisplayName resource="Property_Name_Column_Expression" />
				<Description resource="Property_Desc_Column_Expression" />
			</Property>
		</Properties>
	</Define>

	<!-- Definitions of trigger node properties -->
	<Define name="ElementTriggerProperties" >
		<Properties>
			<TypeDisplayName resource="Type_Trigger" />
			<Property name="TABLE_CATALOG">
				<DisplayName resource="Identifier_Name_Catalog" />
				<Category resource="Category_Identifier" />
				<Description resource="Identifier_Desc_Catalog" />
			</Property>
			<Property name="TABLE_SCHEMA">
				<DisplayName resource="Identifier_Name_Schema" />
				<Category resource="Category_Identifier" />
				<Description resource="Identifier_Desc_Schema" />
			</Property>
			<Property name="TABLE_NAME">
				<DisplayName resource="Identifier_Name_Table" />
				<Category resource="Category_Identifier" />
				<Description resource="Identifier_Desc_Table" />
			</Property>
			<Property name="TRIGGER_NAME">
				<DisplayName resource="Identifier_Name_Name" />
				<Category resource="Category_Identifier" />
				<Description resource="Identifier_Desc_Name" />
			</Property>
			<Property name="TRIGGER_TYPE">
				<DisplayName resource="Property_Name_Trigger_Type" />
				<Description resource="Property_Desc_Trigger_Name" />
				<StandardValues>
					<StandardValue value="1">Before insert</StandardValue>
					<StandardValue value="2">After insert</StandardValue>
					<StandardValue value="3">Before update</StandardValue>
					<StandardValue value="4">After update</StandardValue>
					<StandardValue value="5">Before delete</StandardValue>
					<StandardValue value="6">After delete</StandardValue>
					<StandardValue value="17">Before insert or update</StandardValue>
					<StandardValue value="18">After insert or update</StandardValue>
					<StandardValue value="25">Before insert or delete</StandardValue>
					<StandardValue value="26">After insert or delete</StandardValue>
					<StandardValue value="27">Before update or delete</StandardValue>
					<StandardValue value="28">After update or delete</StandardValue>
					<StandardValue value="113">Before insert, update or delete</StandardValue>
					<StandardValue value="114">After insert, update or delete</StandardValue>
					<StandardValue value="8192">On connect</StandardValue>
					<StandardValue value="8193">On disconnect</StandardValue>
					<StandardValue value="8194">On transaction begin</StandardValue>
					<StandardValue value="8195">On transaction commit</StandardValue>
					<StandardValue value="8196">On transaction rollback"</StandardValue>
				</StandardValues>
			</Property>
			<Property name="SEQUENCENO">
				<DisplayName resource="Property_Name_Trigger_Sequence" />
				<Description resource="Property_Desc_Trigger_Sequence" />
			</Property>
			<Property name="IS_INACTIVE">
				<DisplayName resource="Property_Name_Trigger_Active" />
				<Description resource="Property_Desc_Trigger_Active" />
				<StandardValues>
					<StandardValue value="0">True</StandardValue>
					<StandardValue value="1">False</StandardValue>
				</StandardValues>
			</Property>
			<Property name="DESCRIPTION">
				<DisplayName resource="Property_Name_Description" />
				<Description resource="Property_Desc_Description" />
			</Property>
			<Property name="EXPRESSION">
				<DisplayName resource="Property_Name_Dsl" />
				<Description resource="Property_Desc_Dsl" />
			</Property>
			<Property name="IS_AUTOINCREMENT">
				<DisplayName resource="Property_Name_Trigger_IsAutoIncrement" />
				<Description resource="Property_Desc_Trigger_IsAutoIncrement" />
			</Property>
			<Property name="DEPENDENT_FIELDS">
				<DisplayName resource="Property_Name_Trigger_Dependencies" />
				<Description resource="Property_Desc_Trigger_Dependencies" />
			</Property>
		</Properties>
	</Define>

	<!-- Definition of table selection node properties -->
	<Define name="ElementSelectionTables" parameters="elementType, subType, isSystemObject" >
		<Selection type="$(elementType)" restrictions="null,null,null" filter="IS_SYSTEM_TABLE = $(isSystemObject)">
			<SelectionNode nodeId="Table">
				<DisplayName>{Table.TABLE_NAME}</DisplayName>
				<Icon name="BlackbirdSql.VisualStudio.Ddex.Resources.ViewSupport.$(subType)Table.ico" assembly="" />
				<Properties>
					<TypeDisplayName resource="Type_Table" />
					<Property name="TABLE_CATALOG">
						<DisplayName resource="Identifier_Name_Catalog" />
						<Category resource="Category_Identifier" />
						<Description resource="Identifier_Desc_Catalog" />
					</Property>
					<Property name="TABLE_SCHEMA">
						<DisplayName resource="Identifier_Name_Schema" />
						<Category resource="Category_Identifier" />
						<Description resource="Identifier_Desc_Schema" />
					</Property>
					<Property name="TABLE_NAME">
						<DisplayName resource="Identifier_Name_Name" />
						<Category resource="Category_Identifier" />
						<Description resource="Identifier_Desc_Name" />
					</Property>
					<Property name="IS_SYSTEM_TABLE">
						<DisplayName resource="Property_Name_Type" />
						<StandardValues>
							<StandardValue value="0" resource="Property_Value_TableType_User" />
							<StandardValue value="1" resource="Property_Value_TableType_System" />
						</StandardValues>
					</Property>
					<Property name="DESCRIPTION">
						<DisplayName resource="Property_Name_Description" />
						<Description resource="Property_Desc_Description" />
					</Property>
				</Properties>
				<Commands>
					<CommandRef name="CommandRetrieveData" />
					<CommandRef name="Command$(subType)NewQuery" />
				</Commands>
				<Children>
					<StaticNode skipWhen="{Table.IS_SYSTEM_TABLE} = 1" nodeId="Indexes">
						<DisplayName resource="Node_Name_Indexes" />
						<!-- <Icon name="BlackbirdSql.VisualStudio.Ddex.Resources.ViewSupport.FolderIndexes.ico" assembly="" /> -->
						<Children>
							<Selection type="Index" restrictions="{Table.TABLE_CATALOG},{Table.TABLE_SCHEMA},{Table.TABLE_NAME}" filter="IS_PRIMARY = true">
								<SelectionNode nodeId="Index">
									<DisplayName>{Index.INDEX_NAME}</DisplayName>
									<Icon name="PrimaryKey" />
									<PropertiesRef name="ElementIndexProperties" />
									<Children>
										<Selection type="IndexColumn" restrictions="{Index.TABLE_CATALOG},{Index.TABLE_SCHEMA},{Index.TABLE_NAME},{Index.INDEX_NAME}">
											<SelectionNode nodeId="IndexColumn">
												<DisplayName>{IndexColumn.COLUMN_NAME}</DisplayName>
												<Icon name="BlackbirdSql.VisualStudio.Ddex.Resources.ViewSupport.ComputedColumn.ico" when="{IndexColumn.COMPUTED}" assembly="" />
												<Icon name="Column" />
												<PropertiesRef name="ElementIndexColumnProperties" />
											</SelectionNode>
										</Selection>
									</Children>
								</SelectionNode>
							</Selection>
							<Selection type="Index" restrictions="{Table.TABLE_CATALOG},{Table.TABLE_SCHEMA},{Table.TABLE_NAME}"  filter="IS_PRIMARY = false AND IS_UNIQUE = true">
								<SelectionNode nodeId="Index">
									<DisplayName>{Index.INDEX_NAME}</DisplayName>
									<Icon name="UniqueKey" />
									<PropertiesRef name="ElementIndexProperties" />
									<Children>
										<Selection type="IndexColumn" restrictions="{Index.TABLE_CATALOG},{Index.TABLE_SCHEMA},{Index.TABLE_NAME},{Index.INDEX_NAME}">
											<SelectionNode nodeId="IndexColumn">
												<DisplayName>{IndexColumn.COLUMN_NAME}</DisplayName>
												<Icon name="BlackbirdSql.VisualStudio.Ddex.Resources.ViewSupport.ComputedColumn.ico" when="{IndexColumn.COMPUTED}" assembly="" />
												<Icon name="Column" />
												<PropertiesRef name="ElementIndexColumnProperties" />
											</SelectionNode>
										</Selection>
									</Children>
								</SelectionNode>
							</Selection>
							<Selection type="Index" restrictions="{Table.TABLE_CATALOG},{Table.TABLE_SCHEMA},{Table.TABLE_NAME}"  filter="IS_PRIMARY = false AND IS_UNIQUE = false AND IS_FOREIGNKEY = false">
								<SelectionNode nodeId="Index">
									<DisplayName>{Index.INDEX_NAME}</DisplayName>
									<Icon name="Index" />
									<PropertiesRef name="ElementIndexProperties" />
									<Children>
										<Selection type="IndexColumn" restrictions="{Index.TABLE_CATALOG},{Index.TABLE_SCHEMA},{Index.TABLE_NAME},{Index.INDEX_NAME}">
											<SelectionNode nodeId="IndexColumn">
												<DisplayName>{IndexColumn.COLUMN_NAME}</DisplayName>
												<Icon name="BlackbirdSql.VisualStudio.Ddex.Resources.ViewSupport.ComputedColumn.ico" when="{IndexColumn.COMPUTED}" assembly="" />
												<Icon name="Column" />
												<PropertiesRef name="ElementIndexColumnProperties" />
											</SelectionNode>
										</Selection>
									</Children>
								</SelectionNode>
							</Selection>
							<Selection type="Index" restrictions="{Table.TABLE_CATALOG},{Table.TABLE_SCHEMA},{Table.TABLE_NAME}"  filter="IS_FOREIGNKEY = true">
								<SelectionNode nodeId="Index">
									<DisplayName>{Index.INDEX_NAME}</DisplayName>
									<Icon name="ForeignKey" />
									<PropertiesRef name="ElementIndexProperties" />
									<Children>
										<Selection type="IndexColumn" restrictions="{Index.TABLE_CATALOG},{Index.TABLE_SCHEMA},{Index.TABLE_NAME},{Index.INDEX_NAME}">
											<SelectionNode nodeId="IndexColumn">
												<DisplayName>{IndexColumn.COLUMN_NAME}</DisplayName>
												<Icon name="BlackbirdSql.VisualStudio.Ddex.Resources.ViewSupport.ComputedColumn.ico" when="{IndexColumn.COMPUTED}" assembly="" />
												<Icon name="Column" />
												<PropertiesRef name="ElementIndexColumnProperties" />
											</SelectionNode>
										</Selection>
									</Children>
								</SelectionNode>
							</Selection>
						</Children>
					</StaticNode>
					<StaticNode skipWhen="{Table.IS_SYSTEM_TABLE} = 1" nodeId="ForeignKeys">
						<DisplayName resource="Node_Name_ForeignKeys" />
						<!-- <Icon name="BlackbirdSql.VisualStudio.Ddex.Resources.ViewSupport.FolderForeignKeys.ico" assembly="" /> -->
						<Children>
							<Selection type="TableForeignKey" restrictions="{Table.TABLE_CATALOG},{Table.TABLE_SCHEMA},{Table.TABLE_NAME}">
								<SelectionNode nodeId="ForeignKey">
									<DisplayName>{ForeignKey.CONSTRAINT_NAME}</DisplayName>
									<Icon name="ForeignKey" />
									<Properties>
										<TypeDisplayName resource="Type_ForeignKey" />
										<Property name="CONSTRAINT_CATALOG">
											<DisplayName resource="Identifier_Name_Catalog" />
											<Category resource="Category_Identifier" />
											<Description resource="Identifier_Desc_Catalog" />
										</Property>
										<Property name="CONSTRAINT_SCHEMA">
											<DisplayName resource="Identifier_Name_Schema" />
											<Category resource="Category_Identifier" />
											<Description resource="Identifier_Desc_Schema" />
										</Property>
										<Property name="TABLE_NAME">
											<DisplayName resource="Identifier_Name_Table" />
											<Category resource="Category_Identifier" />
											<Description resource="Identifier_Desc_Table" />
										</Property>
										<Property name="CONSTRAINT_NAME">
											<DisplayName resource="Identifier_Name_Name" />
											<Category resource="Category_Identifier" />
											<Description resource="Identifier_Desc_Name" />
										</Property>
										<Property name="REFERENCED_TABLE_CATALOG">
											<DisplayName resource="Property_Name_ForeignKey_ReferencedCatalog" />
											<Category resource="Category_ReferencedTable" />
											<Description resource="Property_Desc_ForeignKey_ReferencedCatalog" />
										</Property>
										<Property name="REFERENCED_TABLE_SCHEMA">
											<DisplayName resource="Property_Name_ForeignKey_ReferencedSchema" />
											<Category resource="Category_ReferencedTable" />
											<Description resource="Property_Desc_ForeignKey_ReferencedSchema" />
										</Property>
										<Property name="REFERENCED_TABLE_NAME">
											<DisplayName resource="Property_Name_ForeignKey_ReferencedTable" />
											<Category resource="Category_ReferencedTable" />
											<Description resource="Property_Desc_ForeignKey_ReferencedTable" />
										</Property>
										<Property name="UPDATE_RULE">
											<DisplayName resource="Property_Name_ForeignKey_UpdateRule" />
											<Description resource="Property_Desc_ForeignKey_UpdateRule" />
										</Property>
										<Property name="DELETE_RULE">
											<DisplayName resource="Property_Name_ForeignKey_DeleteRule" />
											<Description resource="Property_Desc_ForeignKey_DeleteRule" />
										</Property>
									</Properties>
									<Children>
										<Selection type="TableForeignKeyColumn" restrictions="{ForeignKey.CONSTRAINT_CATALOG},{ForeignKey.CONSTRAINT_SCHEMA},{ForeignKey.TABLE_NAME},{ForeignKey.CONSTRAINT_NAME}">
											<SelectionNode nodeId="ForeignKeyColumn">
												<DisplayName>{ForeignKeyColumn.COLUMN_NAME}</DisplayName>
												<Icon name="Column" />
												<Properties>
													<TypeDisplayName resource="Type_ForeignKeyColumn" />
													<Property name="CONSTRAINT_CATALOG">
														<DisplayName resource="Identifier_Name_Catalog" />
														<Category resource="Category_Identifier" />
														<Description resource="Identifier_Desc_Catalog" />
													</Property>
													<Property name="CONSTRAINT_SCHEMA">
														<DisplayName resource="Identifier_Name_Schema" />
														<Category resource="Category_Identifier" />
														<Description resource="Identifier_Desc_Schema" />
													</Property>
													<Property name="TABLE_NAME">
														<DisplayName resource="Identifier_Name_Table" />
														<Category resource="Category_Identifier" />
														<Description resource="Identifier_Desc_Table" />
													</Property>
													<Property name="CONSTRAINT_NAME">
														<DisplayName resource="Identifier_Name_ForeignKey" />
														<Category resource="Category_Identifier" />
														<Description resource="Identifier_Desc_ForeignKey" />
													</Property>
													<Property name="COLUMN_NAME">
														<DisplayName resource="Identifier_Name_Name" />
														<Category resource="Category_Identifier" />
														<Description resource="Identifier_Desc_Name" />
													</Property>
													<Property name="REFERENCED_TABLE_CATALOG">
														<DisplayName resource="Property_Name_ForeignKeyColumn_ReferencedCatalog" />
														<Category resource="Category_ReferencedColumn" />
														<Description resource="Property_Desc_ForeignKeyColumn_ReferencedCatalog" />
													</Property>
													<Property name="REFERENCED_TABLE_SCHEMA">
														<DisplayName resource="Property_Name_ForeignKeyColumn_ReferencedSchema" />
														<Category resource="Category_ReferencedColumn" />
														<Description resource="Property_Desc_ForeignKeyColumn_ReferencedSchema" />
													</Property>
													<Property name="REFERENCED_TABLE_NAME">
														<DisplayName resource="Property_Name_ForeignKeyColumn_ReferencedTable" />
														<Category resource="Category_ReferencedColumn" />
														<Description resource="Property_Desc_ForeignKeyColumn_ReferencedTable" />
													</Property>
													<Property name="REFERENCED_COLUMN_NAME">
														<DisplayName resource="Property_Name_ForeignKeyColumn_ReferencedColumn" />
														<Category resource="Category_ReferencedColumn" />
														<Description resource="Property_Desc_ForeignKeyColumn_ReferencedColumn" />
													</Property>
													<Property name="ORDINAL_POSITION">
														<DisplayName resource="Property_Name_ForeignKeyColumn_Ordinal" />
														<Description resource="Property_Desc_ForeignKeyColumn_Ordinal" />
													</Property>
												</Properties>
											</SelectionNode>
										</Selection>
									</Children>
								</SelectionNode>
							</Selection>
						</Children>
					</StaticNode>
					<StaticNode skipWhen="{Table.IS_SYSTEM_TABLE} = 1" nodeId="Triggers">
						<DisplayName resource="Node_Name_Triggers" />
						<!-- <Icon name="BlackbirdSql.VisualStudio.Ddex.Resources.ViewSupport.FolderTriggers.ico" assembly="" /> -->
						<Children>
							<Selection type="Trigger" restrictions="{Table.TABLE_CATALOG},{Table.TABLE_SCHEMA},{Table.TABLE_NAME}" filter="IS_AUTOINCREMENT = true">
								<SelectionNode nodeId="Trigger">
									<DisplayName>{Trigger.TRIGGER_NAME}</DisplayName>
									<Icon name="BlackbirdSql.VisualStudio.Ddex.Resources.ViewSupport.AutoIncrementColumn.ico" assembly="" />
									<PropertiesRef name="ElementTriggerProperties" />
								</SelectionNode>
							</Selection>
							<Selection type="Trigger" restrictions="{Table.TABLE_CATALOG},{Table.TABLE_SCHEMA},{Table.TABLE_NAME}" filter="IS_AUTOINCREMENT = false">
								<SelectionNode nodeId="Trigger">
									<DisplayName>{Trigger.TRIGGER_NAME}</DisplayName>
									<Icon name="Trigger" />
									<PropertiesRef name="ElementTriggerProperties" />
								</SelectionNode>
							</Selection>
						</Children>
					</StaticNode>
					<!--
					<StaticNode skipWhen="{Table.IS_SYSTEM_TABLE} = 1" nodeId="Columns">
						<DisplayName resource="Node_Name_Columns" />
						<Icon name="BlackbirdSql.VisualStudio.Ddex.Resources.ViewSupport.DotDotDot.ico" assembly="" />
					</StaticNode>
					-->
					<Selection type="TableColumn" restrictions="{Table.TABLE_CATALOG},{Table.TABLE_SCHEMA},{Table.TABLE_NAME}">
						<SelectionNode nodeId="Column">
							<DisplayName>{Column.COLUMN_NAME}</DisplayName>
							<Icon name="BlackbirdSql.VisualStudio.Ddex.Resources.ViewSupport.ComputedColumn.ico" when="{Column.COMPUTED}" assembly="" />
							<Icon name="BlackbirdSql.VisualStudio.Ddex.Resources.ViewSupport.AutoIncrementColumn.ico" when="{Column.IS_AUTOINCREMENT} AND {Column.IS_UNIQUE}" assembly="" />
							<Icon name="PrimaryKey" when="{Column.IS_UNIQUE}" />
							<Icon name="UniqueKey" when="{Column.IN_PRIMARYKEY}" />
							<Icon name="BlackbirdSql.VisualStudio.Ddex.Resources.ViewSupport.$(subType)Column.ico" assembly="" />
							<PropertiesRef name="Element$(subType)TableColumnProperties" />
						</SelectionNode>
					</Selection>
				</Children>
			</SelectionNode>
		</Selection>
	</Define>

	<!-- Definition of table static node properties -->
	<Define name="ElementStaticNodeTablesTables" parameters="elementType, subType, isSystemObject" >
		<StaticNode nodeId="$(subType)$(elementType)s">
			<DisplayName resource="Node_Name_$(subType)$(elementType)s" />
			<Icon name="BlackbirdSql.VisualStudio.Ddex.Resources.ViewSupport.Folder$(subType)Tables.ico" assembly="" />
			<Children>
				<SelectionRef name="ElementSelectionTables" arguments="$(elementType), $(subType), $(isSystemObject)"/>
			</Children>
		</StaticNode>
	</Define>
</DataViewSupport>
