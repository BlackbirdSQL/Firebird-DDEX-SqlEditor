	<!-- Definition of column properties -->
	<Define name="TplPropertiesColumn" parameters="parentType">
		<Properties>
			<TypeDisplayName resource="Type_$(parentType)Column" />
			<PropertyListRef name="TplPropertiesListStandardColumn" />
			<PropertyListRef name="TplPropertiesList$(parentType)Column" />
		</Properties>
	</Define>


	
	<!-- Definition of column selection node -->
	<Define name="TplSelectionColumn" parameters="type, parentType, systemType, restrictions">
		<Selection type="$(type)" restrictions="$(restrictions)">
			<SelectionNode nodeId="Column">
				<DisplayName>{Column.COLUMN_NAME}</DisplayName>
				<Icon name="BlackbirdSql.VisualStudio.Ddex.Resources.ViewSupport.ComputedColumn.ico" when="{Column.IS_COMPUTED} AND NOT ({Column.PARENT_TYPE} = 'Trigger')" assembly="" />
				<Icon name="BlackbirdSql.VisualStudio.Ddex.Resources.ViewSupport.Computed$(parentType)Column.ico" when="{Column.IS_COMPUTED}" assembly="" />
				<Icon name="BlackbirdSql.VisualStudio.Ddex.Resources.ViewSupport.AutoIncrementColumn.ico" when="{Column.IS_AUTOINCREMENT} AND {Column.IS_UNIQUE} AND NOT ({Column.PARENT_TYPE} = 'Trigger')" assembly="" />
				<Icon name="BlackbirdSql.VisualStudio.Ddex.Resources.ViewSupport.PrimaryKey.ico" when="{Column.IS_UNIQUE} AND NOT ({Column.PARENT_TYPE} = 'Trigger')" assembly="" />
				<Icon name="BlackbirdSql.VisualStudio.Ddex.Resources.ViewSupport.PrimaryKey$(parentType).ico" when="{Column.IS_UNIQUE}" assembly="" />
				<Icon name="BlackbirdSql.VisualStudio.Ddex.Resources.ViewSupport.UniqueKey.ico" when="{Column.IN_PRIMARYKEY} AND NOT ({Column.PARENT_TYPE} = 'Trigger')" assembly="" />
				<Icon name="BlackbirdSql.VisualStudio.Ddex.Resources.ViewSupport.UniqueKey$(parentType).ico" when="{Column.IN_PRIMARYKEY}" assembly="" />
				<Icon name="BlackbirdSql.VisualStudio.Ddex.Resources.ViewSupport.$(systemType)$(parentType)Column.ico" when="{Column.PARENT_TYPE} = 'Trigger' OR {Column.PARENT_TYPE} = 'View'" assembly="" />
				<Icon name="BlackbirdSql.VisualStudio.Ddex.Resources.ViewSupport.$(systemType)Column.ico" assembly="" />
				<PropertiesRef name="TplPropertiesColumn" arguments="$(parentType)"/>
			</SelectionNode>
		</Selection>
	</Define>


	
	<!-- Definition of index node properties -->
	<Define name="TplPropertiesIndex" >
		<Properties>
			<TypeDisplayName resource="Type_Index" />
			<Property name="TABLE_CATALOG">
				<DisplayName resource="Identifier_Name_Catalog" />
				<Category resource="Category_Identifier" />
				<Description resource="Identifier_Desc_Catalog" />
			</Property>
			<Property name="TABLE_SCHEMA">
				<DisplayName resource="Identifier_Name_Schema" />
				<Category resource="Category_Identifier" />
				<Description resource="Identifier_Desc_Schema" />
			</Property>
			<Property name="TABLE_NAME">
				<DisplayName resource="Identifier_Name_Table" />
				<Category resource="Category_Identifier" />
				<Description resource="Identifier_Desc_Table" />
			</Property>
			<Property name="INDEX_NAME">
				<DisplayName resource="Identifier_Name_Name" />
				<Category resource="Category_Identifier" />
				<Description resource="Identifier_Desc_Name" />
			</Property>
			<Property name="IS_PRIMARY">
				<DisplayName resource="Property_Name_Index_IsPrimary" />
				<Description resource="Property_Desc_Index_IsPrimary" />
			</Property>
			<Property name="IS_UNIQUE">
				<DisplayName resource="Property_Name_Index_IsUnique" />
				<Description resource="Property_Desc_Index_IsUnique" />
			</Property>
			<Property name="IS_FOREIGNKEY">
				<DisplayName resource="Property_Name_Index_IsForeignKey" />
				<Description resource="Property_Desc_Index_IsForeignKey" />
			</Property>
			<Property name="IS_SYSTEM_OBJECT">
				<DisplayName resource="Property_Name_Type" />
				<StandardValues>
					<StandardValue value="false" resource="Property_Value_IndexType_User" />
					<StandardValue value="true" resource="Property_Value_IndexType_System" />
				</StandardValues>
			</Property>
			<Property name="EXPRESSION">
				<DisplayName resource="Property_Name_Index_Expression" />
				<Description resource="Property_Desc_Index_Expression" />
			</Property>
			<Property name="IS_COMPUTED">
				<DisplayName resource="Property_Name_Index_IsComputed" />
				<Description resource="Property_Desc_Index_IsComputed" />
			</Property>
		</Properties>
	</Define>


	<!-- Definitions of trigger node properties -->
	<Define name="TplPropertiesTrigger" >
		<Properties>
			<TypeDisplayName resource="Type_Trigger" />
			<Property name="TABLE_CATALOG">
				<DisplayName resource="Identifier_Name_Catalog" />
				<Category resource="Category_Identifier" />
				<Description resource="Identifier_Desc_Catalog" />
			</Property>
			<Property name="TABLE_SCHEMA">
				<DisplayName resource="Identifier_Name_Schema" />
				<Category resource="Category_Identifier" />
				<Description resource="Identifier_Desc_Schema" />
			</Property>
			<Property name="TABLE_NAME">
				<DisplayName resource="Identifier_Name_Table" />
				<Category resource="Category_Identifier" />
				<Description resource="Identifier_Desc_Table" />
			</Property>
			<Property name="TRIGGER_NAME">
				<DisplayName resource="Identifier_Name_Name" />
				<Category resource="Category_Identifier" />
				<Description resource="Identifier_Desc_Name" />
			</Property>
			<Property name="TRIGGER_TYPE">
				<DisplayName resource="Property_Name_Trigger_Type" />
				<Description resource="Property_Desc_Trigger_Name" />
				<StandardValues>
					<StandardValue value="1">Before insert</StandardValue>
					<StandardValue value="2">After insert</StandardValue>
					<StandardValue value="3">Before update</StandardValue>
					<StandardValue value="4">After update</StandardValue>
					<StandardValue value="5">Before delete</StandardValue>
					<StandardValue value="6">After delete</StandardValue>
					<StandardValue value="17">Before insert or update</StandardValue>
					<StandardValue value="18">After insert or update</StandardValue>
					<StandardValue value="25">Before insert or delete</StandardValue>
					<StandardValue value="26">After insert or delete</StandardValue>
					<StandardValue value="27">Before update or delete</StandardValue>
					<StandardValue value="28">After update or delete</StandardValue>
					<StandardValue value="113">Before insert, update or delete</StandardValue>
					<StandardValue value="114">After insert, update or delete</StandardValue>
					<StandardValue value="8192">On connect</StandardValue>
					<StandardValue value="8193">On disconnect</StandardValue>
					<StandardValue value="8194">On transaction begin</StandardValue>
					<StandardValue value="8195">On transaction commit</StandardValue>
					<StandardValue value="8196">On transaction rollback"</StandardValue>
				</StandardValues>
			</Property>
			<Property name="SEQUENCENO">
				<DisplayName resource="Property_Name_Trigger_Sequence" />
				<Description resource="Property_Desc_Trigger_Sequence" />
			</Property>
			<Property name="IS_INACTIVE">
				<DisplayName resource="Property_Name_Trigger_Active" />
				<Description resource="Property_Desc_Trigger_Active" />
				<StandardValues>
					<StandardValue value="0">True</StandardValue>
					<StandardValue value="1">False</StandardValue>
				</StandardValues>
			</Property>
			<Property name="DESCRIPTION">
				<DisplayName resource="Property_Name_Description" />
				<Description resource="Property_Desc_Description" />
			</Property>
			<Property name="EXPRESSION">
				<DisplayName resource="Property_Name_Dsl" />
				<Description resource="Property_Desc_Dsl" />
			</Property>
			<Property name="IS_AUTOINCREMENT">
				<DisplayName resource="Property_Name_Trigger_IsAutoIncrement" />
				<Description resource="Property_Desc_Trigger_IsAutoIncrement" />
			</Property>
			<Property name="DEPENDENCY_FIELDS">
				<DisplayName resource="Property_Name_Trigger_Dependencies" />
				<Description resource="Property_Desc_Trigger_Dependencies" />
			</Property>
		</Properties>
	</Define>

	
	
	<!-- Definition of trigger selection node without columns -->
	<Define name="TplSelectionTriggersWithChildren" parameters="triggerType, systemType, isSystemObject, restrictions" >
		<Selection type="$(triggerType)" restrictions="$(restrictions)" >
			<!-- filter="IS_SYSTEM_OBJECT = $(isSystemObject)" > -->
			<SelectionNode nodeId="Trigger">
				<DisplayName>{Trigger.TRIGGER_NAME}</DisplayName>
				<Icon name="BlackbirdSql.VisualStudio.Ddex.Resources.ViewSupport.AutoIncrement$(systemType)Trigger.ico" when="{Trigger.IS_AUTOINCREMENT}" assembly="" />
				<Icon name="BlackbirdSql.VisualStudio.Ddex.Resources.ViewSupport.$(systemType)Trigger.ico" assembly="" />
				<PropertiesRef name="TplPropertiesTrigger" />
				<Children>
					<SelectionRef name="TplSelectionColumn" arguments="TriggerColumn,Trigger,$(systemType),{Trigger.TABLE_CATALOG},,{Trigger.TABLE_SCHEMA},,{Trigger.TRIGGER_NAME}" />
				</Children>
			</SelectionNode>
		</Selection>
	</Define>

	
	<!-- Definition of trigger selection node with columns -->
	<Define name="TplSelectionTriggersNoChildren" parameters="triggerType, systemType, isSystemObject, restrictions" >
		<Selection type="$(triggerType)" restrictions="$(restrictions)" >
			<!-- filter="IS_SYSTEM_OBJECT = $(isSystemObject)" > -->
			<SelectionNode nodeId="Trigger">
				<DisplayName>{Trigger.TRIGGER_NAME}</DisplayName>
				<Icon name="BlackbirdSql.VisualStudio.Ddex.Resources.ViewSupport.AutoIncrement$(systemType)Trigger.ico" when="{Trigger.IS_AUTOINCREMENT}" assembly="" />
				<Icon name="BlackbirdSql.VisualStudio.Ddex.Resources.ViewSupport.$(systemType)Trigger.ico" assembly="" />
				<PropertiesRef name="TplPropertiesTrigger" />
			</SelectionNode>
		</Selection>
	</Define>

	
	<!-- Definition of trigger static node -->
	<Define name="TplStaticNodeTriggers" parameters="triggerType, triggerChildrenType, systemType, isSystemObject" >
		<StaticNode nodeId="$(systemType)$(triggerType)s">
			<DisplayName resource="Node_Name_$(systemType)$(triggerType)s" />
			<Icon name="BlackbirdSql.VisualStudio.Ddex.Resources.ViewSupport.Folder$(systemType)$(triggerType)s.ico" assembly="" />
			<Children>
				<SelectionRef name="TplSelectionTriggers$(triggerChildrenType)" arguments="$(triggerType),$(systemType),$(isSystemObject),null,,null"/>
			</Children>
		</StaticNode>
	</Define>

	

	<!-- Definition of table selection node -->
	<Define name="TplSelectionTables" parameters="systemType, isSystemObject, triggerChildrenType" >
		<Selection type="Table" restrictions="null,null" filter="IS_SYSTEM_OBJECT = $(isSystemObject)">
			<SelectionNode nodeId="Table">
				<DisplayName>{Table.TABLE_NAME}</DisplayName>
				<Icon name="BlackbirdSql.VisualStudio.Ddex.Resources.ViewSupport.$(systemType)Table.ico" assembly="" />
				<Properties>
					<TypeDisplayName resource="Type_Table" />
					<Property name="TABLE_CATALOG">
						<DisplayName resource="Identifier_Name_Catalog" />
						<Category resource="Category_Identifier" />
						<Description resource="Identifier_Desc_Catalog" />
					</Property>
					<Property name="TABLE_SCHEMA">
						<DisplayName resource="Identifier_Name_Schema" />
						<Category resource="Category_Identifier" />
						<Description resource="Identifier_Desc_Schema" />
					</Property>
					<Property name="TABLE_NAME">
						<DisplayName resource="Identifier_Name_Name" />
						<Category resource="Category_Identifier" />
						<Description resource="Identifier_Desc_Name" />
					</Property>
					<Property name="IS_SYSTEM_OBJECT">
						<DisplayName resource="Property_Name_Type" />
						<StandardValues>
							<StandardValue value="false" resource="Property_Value_TableType_User" />
							<StandardValue value="true" resource="Property_Value_TableType_System" />
						</StandardValues>
					</Property>
					<Property name="DESCRIPTION">
						<DisplayName resource="Property_Name_Description" />
						<Description resource="Property_Desc_Description" />
					</Property>
					<Property name="INDEX_COUNT">
						<DisplayName resource="Property_Name_IndexCount" />
						<Description resource="Property_Desc_IndexCount" />
					</Property>
					<Property name="FOREIGNKEY_COUNT">
						<DisplayName resource="Property_Name_ForeignKeyCount" />
						<Description resource="Property_Desc_ForeignKeyCount" />
					</Property>
					<Property name="TRIGGER_COUNT">
						<DisplayName resource="Property_Name_TriggerCount" />
						<Description resource="Property_Desc_TriggrCount" />
					</Property>
				</Properties>
				<Commands>
					<CommandRef name="CommandRetrieveData" />
					<CommandRef name="Command$(systemType)NewQuery" />
				</Commands>
				<Children>
					<StaticNode skipWhen="{Table.INDEX_COUNT} = 0" nodeId="Indexes">
						<DisplayName resource="Node_Name_Indexes" />
						<!-- <Icon name="BlackbirdSql.VisualStudio.Ddex.Resources.ViewSupport.FolderIndexes.ico" assembly="" /> -->
						<Children>
							<Selection type="Index" restrictions="{Table.TABLE_CATALOG},{Table.TABLE_SCHEMA},{Table.TABLE_NAME}" filter="IS_PRIMARY = true">
								<SelectionNode nodeId="Index">
									<DisplayName>{Index.INDEX_NAME}</DisplayName>
									<Icon name="PrimaryKey" />
									<PropertiesRef name="TplPropertiesIndex" />
									<Children>
										<SelectionRef name="TplSelectionColumn" arguments="IndexColumn,Index,$(systemType),{Index.TABLE_CATALOG},,{Index.TABLE_SCHEMA},,{Index.TABLE_NAME},,{Index.INDEX_NAME}" />
									</Children>
								</SelectionNode>
							</Selection>
							<Selection type="Index" restrictions="{Table.TABLE_CATALOG},{Table.TABLE_SCHEMA},{Table.TABLE_NAME}"  filter="IS_PRIMARY = false AND IS_UNIQUE = true">
								<SelectionNode nodeId="Index">
									<DisplayName>{Index.INDEX_NAME}</DisplayName>
									<Icon name="UniqueKey" />
									<PropertiesRef name="TplPropertiesIndex" />
									<Children>
										<SelectionRef name="TplSelectionColumn" arguments="IndexColumn,Index,$(systemType),{Index.TABLE_CATALOG},,{Index.TABLE_SCHEMA},,{Index.TABLE_NAME},,{Index.INDEX_NAME}" />
									</Children>
								</SelectionNode>
							</Selection>
							<Selection type="Index" restrictions="{Table.TABLE_CATALOG},{Table.TABLE_SCHEMA},{Table.TABLE_NAME}"  filter="IS_PRIMARY = false AND IS_UNIQUE = false AND IS_FOREIGNKEY = false">
								<SelectionNode nodeId="Index">
									<DisplayName>{Index.INDEX_NAME}</DisplayName>
									<Icon name="Index" />
									<PropertiesRef name="TplPropertiesIndex" />
									<Children>
										<SelectionRef name="TplSelectionColumn" arguments="IndexColumn,Index,$(systemType),{Index.TABLE_CATALOG},,{Index.TABLE_SCHEMA},,{Index.TABLE_NAME},,{Index.INDEX_NAME}" />
									</Children>
								</SelectionNode>
							</Selection>
							<Selection type="Index" restrictions="{Table.TABLE_CATALOG},{Table.TABLE_SCHEMA},{Table.TABLE_NAME}"  filter="IS_FOREIGNKEY = true">
								<SelectionNode nodeId="Index">
									<DisplayName>{Index.INDEX_NAME}</DisplayName>
									<Icon name="ForeignKey" />
									<PropertiesRef name="TplPropertiesIndex" />
									<Children>
										<SelectionRef name="TplSelectionColumn" arguments="IndexColumn,Index,$(systemType),{Index.TABLE_CATALOG},,{Index.TABLE_SCHEMA},,{Index.TABLE_NAME},,{Index.INDEX_NAME}" />
									</Children>
								</SelectionNode>
							</Selection>
						</Children>
					</StaticNode>
					<StaticNode skipWhen="{Table.INDEX_COUNT} > 0" nodeId="IndexesEmpty">
						<DisplayName resource="Node_Name_Indexes" />
					</StaticNode>
					<StaticNode skipWhen="{Table.IS_SYSTEM_OBJECT} OR {Table.FOREIGNKEY_COUNT} = 0" nodeId="ForeignKeys">
						<DisplayName resource="Node_Name_ForeignKeys" />
						<!-- <Icon name="BlackbirdSql.VisualStudio.Ddex.Resources.ViewSupport.FolderForeignKeys.ico" assembly="" /> -->
						<Children>
							<Selection type="ForeignKey" restrictions="{Table.TABLE_CATALOG},{Table.TABLE_SCHEMA},{Table.TABLE_NAME}">
								<SelectionNode nodeId="ForeignKey">
									<DisplayName>{ForeignKey.CONSTRAINT_NAME}</DisplayName>
									<Icon name="ForeignKey" />
									<Properties>
										<TypeDisplayName resource="Type_ForeignKey" />
										<Property name="CONSTRAINT_CATALOG">
											<DisplayName resource="Identifier_Name_Catalog" />
											<Category resource="Category_Identifier" />
											<Description resource="Identifier_Desc_Catalog" />
										</Property>
										<Property name="CONSTRAINT_SCHEMA">
											<DisplayName resource="Identifier_Name_Schema" />
											<Category resource="Category_Identifier" />
											<Description resource="Identifier_Desc_Schema" />
										</Property>
										<Property name="TABLE_NAME">
											<DisplayName resource="Identifier_Name_Table" />
											<Category resource="Category_Identifier" />
											<Description resource="Identifier_Desc_Table" />
										</Property>
										<Property name="CONSTRAINT_NAME">
											<DisplayName resource="Identifier_Name_Name" />
											<Category resource="Category_Identifier" />
											<Description resource="Identifier_Desc_Name" />
										</Property>
										<Property name="REFERENCED_TABLE_CATALOG">
											<DisplayName resource="Property_Name_ForeignKey_ReferencedCatalog" />
											<Category resource="Category_ReferencedTable" />
											<Description resource="Property_Desc_ForeignKey_ReferencedCatalog" />
										</Property>
										<Property name="REFERENCED_TABLE_SCHEMA">
											<DisplayName resource="Property_Name_ForeignKey_ReferencedSchema" />
											<Category resource="Category_ReferencedTable" />
											<Description resource="Property_Desc_ForeignKey_ReferencedSchema" />
										</Property>
										<Property name="REFERENCED_TABLE_NAME">
											<DisplayName resource="Property_Name_ForeignKey_ReferencedTable" />
											<Category resource="Category_ReferencedTable" />
											<Description resource="Property_Desc_ForeignKey_ReferencedTable" />
										</Property>
										<Property name="UPDATE_RULE">
											<DisplayName resource="Property_Name_ForeignKey_UpdateRule" />
											<Description resource="Property_Desc_ForeignKey_UpdateRule" />
										</Property>
										<Property name="DELETE_RULE">
											<DisplayName resource="Property_Name_ForeignKey_DeleteRule" />
											<Description resource="Property_Desc_ForeignKey_DeleteRule" />
										</Property>
									</Properties>
									<Children>
										<SelectionRef name="TplSelectionColumn" arguments="ForeignKeyColumn,ForeignKey,$(systemType),{ForeignKey.CONSTRAINT_CATALOG},,{ForeignKey.CONSTRAINT_SCHEMA},,{ForeignKey.TABLE_NAME},,{ForeignKey.CONSTRAINT_NAME}" />
									</Children>
								</SelectionNode>
							</Selection>
						</Children>
					</StaticNode>
					<StaticNode skipWhen="{Table.IS_SYSTEM_OBJECT} OR {Table.FOREIGNKEY_COUNT} > 0" nodeId="ForeignKeysEmpty">
						<DisplayName resource="Node_Name_ForeignKeys" />
					</StaticNode>
					<StaticNode  skipWhen="{Table.IS_SYSTEM_OBJECT} OR {Table.TRIGGER_COUNT} = 0" nodeId="Triggers">
						<DisplayName resource="Node_Name_Triggers" />
						<!-- <Icon name="BlackbirdSql.VisualStudio.Ddex.Resources.ViewSupport.FolderTriggers.ico" assembly="" /> -->
						<Children>
							<SelectionRef name="TplSelectionTriggers$(triggerChildrenType)" arguments="AutoIncrementTrigger,$(systemType),$(isSystemObject),{Table.TABLE_CATALOG},,{Table.TABLE_SCHEMA},,{Table.TABLE_NAME}"/>
							<SelectionRef name="TplSelectionTriggers$(triggerChildrenType)" arguments="StandardTrigger,$(systemType),$(isSystemObject),{Table.TABLE_CATALOG},,{Table.TABLE_SCHEMA},,{Table.TABLE_NAME}"/>
						</Children>
					</StaticNode>
					<StaticNode  skipWhen="{Table.IS_SYSTEM_OBJECT} = false OR {Table.TRIGGER_COUNT} = 0" nodeId="Triggers">
						<DisplayName resource="Node_Name_Triggers" />
						<!-- <Icon name="BlackbirdSql.VisualStudio.Ddex.Resources.ViewSupport.FolderTriggers.ico" assembly="" /> -->
						<Children>
							<SelectionRef name="TplSelectionTriggers$(triggerChildrenType)" arguments="SystemTrigger,$(systemType),$(isSystemObject),{Table.TABLE_CATALOG},,{Table.TABLE_SCHEMA},,{Table.TABLE_NAME}"/>
						</Children>
					</StaticNode>
					<StaticNode  skipWhen="{Table.TRIGGER_COUNT} > 0" nodeId="TriggersEmpty">
						<DisplayName resource="Node_Name_Triggers" />
					</StaticNode>
					<SelectionRef name="TplSelectionColumn" arguments="Column,Table,$(systemType),{Table.TABLE_CATALOG},,{Table.TABLE_SCHEMA},,{Table.TABLE_NAME}" />
				</Children>
			</SelectionNode>
		</Selection>
	</Define>

	
	<!-- Definition of table static node -->
	<Define name="TplStaticNodeTablesTables" parameters="systemType, isSystemObject, triggerChildrenType" >
		<StaticNode nodeId="$(systemType)Tables">
			<DisplayName resource="Node_Name_$(systemType)Tables" />
			<Icon name="BlackbirdSql.VisualStudio.Ddex.Resources.ViewSupport.Folder$(systemType)Tables.ico" assembly="" />
			<Children>
				<SelectionRef name="TplSelectionTables" arguments="$(systemType),$(isSystemObject),$(triggerChildrenType)"/>
			</Children>
		</StaticNode>
	</Define>
</DataViewSupport>
