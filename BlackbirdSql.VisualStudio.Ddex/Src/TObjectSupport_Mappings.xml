					<Conversion>
						<CallMapper />
					</Conversion>
				</Property>
				<Property name="AdoDotNetDbType" underlyingMember="DOMAIN_DATA_TYPE">
					<Conversion>
						<CallMapper />
					</Conversion>
				</Property>
				<Property name="FrameworkDataType" underlyingMember="DOMAIN_DATA_TYPE">
					<Conversion>
						<CallMapper />
					</Conversion>
				</Property>
