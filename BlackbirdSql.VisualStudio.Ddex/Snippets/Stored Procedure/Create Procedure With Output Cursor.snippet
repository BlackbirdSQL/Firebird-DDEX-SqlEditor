<?xml version="1.0" encoding="utf-8" ?>
<CodeSnippets xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <CodeSnippet Format="1.0.0">
    <Header>
      <Title>Create Procedure with CURSOR OUTPUT Parameter</Title>
      <Shortcut></Shortcut>
      <Description>Creates a stored procedure that has a Cursor output parameter. (Not yet in Firebird syntax)</Description>
      <Author>BlackbirdSql</Author>
      <SnippetTypes>
        <SnippetType>Expansion</SnippetType>
      </SnippetTypes>
    </Header>
    <Snippet>
      <Declarations>
        <Literal>
          <ID>storedprocname</ID>
          <ToolTip>Name of the stored procedure</ToolTip>
          <Default>Sample_Procedure</Default>
        </Literal>
        <Literal>
          <ID>Param1</ID>
          <ToolTip>Output parameter name</ToolTip>
          <Default>sample_procedure_cursor</Default>
        </Literal>
        <Literal>
          <ID>datatype1</ID>
          <ToolTip>Datatype of the output parameter</ToolTip>
          <Default>CURSOR</Default>
        </Literal>
      </Declarations>
      <Code Language="SQL_FIREBIRD">
        <![CDATA[CREATE PROCEDURE $storedprocname$ 
    @$Param1$ $datatype1$ VARYING OUTPUT
AS
    SET @$Param1$ = CURSOR FOR
        select 1
    OPEN @$Param1$ 
RETURN 0 $end$]]>
      </Code>
    </Snippet>
  </CodeSnippet>
</CodeSnippets>

